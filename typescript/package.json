    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "nodemon",
    "serve": "node --enable-source-maps build/server.js",
    "build": "npm run esbuild -- --platform=node src/server.ts && npm run esbuild -- --format=cjs src/index.tsx",
    "esbuild": "esbuild --sourcemap --bundle --define:process.env.NODE_ENV=\\\"${NODE_ENV}\\\" --minify --outdir=build",
    "test": "jest --verbose",
    "lint": "npm run prettier && npm run eslint && npm run ts-check",
    "prettier": "prettier --list-different --write './**/*.(ts|tsx|mustache)' '*.js'",
    "eslint": "eslint . --ext .ts --ext .tsx --fix",
    "ts-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
